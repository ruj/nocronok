generator client {
  provider = "prisma-client-js"
}

datasource mysql {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Config {
  id          Int     @id @unique @default(autoincrement())
  module      String  @mysql.VarChar(32)
  name        String  @mysql.VarChar(32)
  value       String?
  description String?

  @@map("config")
}
